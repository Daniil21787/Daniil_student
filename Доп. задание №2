def print_hi(name):
    # Use a breakpoint in the code line below to debug your script.
    print(f'Hi, {name}')  # Press Ctrl+F8 to toggle the breakpoint.


# Press the green button in the gutter to run the script.
if __name__ == '__main__':
    print_hi('PyCharm')

# See PyCharm help at https://www.jetbrains.com/help/pycharm/


def two_numbers(number):
    pass_ = list()
    comb_ = number + 1 // 2
    for i in range(1, comb_):
        j = i + 1
        while i + j <= number:
            if number % (i + j) == 0:
                pass_numbers = str(i) + str(j)
                pass_.append(pass_numbers)
            j += 1
    return pass_
password = True
while password:
    input_number = int(input('Введите число от 3 до 20: '))
    if input_number in range(3, 21):
        password = False
        continue
print(*two_numbers(input_number), ' - верный пароль')